/* pages/PersonalCode/PersonalCode.wxss */
@import "/styles/secureCodePreview.wxss";
#personal-code-container {
    margin: 0 16px;
    margin-top: 20px;
    padding-bottom: 80px;
}
#personal-code-heading-container {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 19px;
}
#compact-id-display {
    padding-top:15px;
    display:flex;
    flex-direction: column;
    align-items: center;
}
#compact-id-display-title {
    font-size:22px;
    font-weight:700;
    margin-bottom:3px;
}
#compact-id-display-id {
    font-size: 34px;
    font-weight: 800;
}
#personal-code-code-container {
    margin-top:16px;
}